server {
  client_max_body_size 4G;
  server_name munin.obsqure.net;
  access_log /var/log/nginx/munin-access.log combined;
  error_log /var/log/nginx/munin-error.log;

  keepalive_timeout 5;

  location / {
    root /var/cache/munin/www;
  }

  location /nginx_status {
    stub_status on;
    access_log   off;
  }
}
